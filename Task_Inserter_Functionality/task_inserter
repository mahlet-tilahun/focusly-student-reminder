

import datetime

Class Task:
    def __init__(self, name, description, deadline, priority):
         self.name = name
         self.description = description
         self.deadline = deadline
         self.priority = priority
   
     def __str__(self):
         return f"Task: {self.name}, Priority: {self.priority},, Deadline: {self.deadline}"

def insert_task(name, description, deadline, priority):
    """
    Insert a new task with the given deatails.
    """
   
    task = Task(name, description, deadline, priority)
   # Here you can add functionality to save the task

   return task
